{
  "name": "Hexagone for Twitter",
  "description": "Make hexagons circles again",
  "version": "0.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["styles/contentscript.css"],
      "js": ["scripts/contentscript.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
